		do{
			System.out.println("로그인 화면입니다.");
			System.out.println("아이디와 비밀번호를 입력하세요.");
			System.out.print("아이디 : ");

			
			System.out.print("비밀번호 : ");
			

			
			if(       ;      ){
				System.out.println("아이디나 비밀번호가 일치하지 않습니다.");
			}
			else{
				System.out.println("로그인한 사용자 정보");
				
			}
			
		} while(!isStop);


	}

}
